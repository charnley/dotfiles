{
    "For Entries": {
        "prefix": "forobj",
        "body": [
            "for (const [${1:key}, ${2:value}] of Object.entries(${3:object})) {",
            "   ${4}",
            "}"
        ],
        "description": "for key value entries in dict"
    },
    "const func": {
        "prefix": "confunc",
        "body": [
            "const ${1:name} = (${2}) => {",
            "   ${3}",
            "}"
        ],
        "description": "for key value entries in dict"
    },
    "const func async": {
        "prefix": "confuncasync",
        "body": [
            "const ${1:name} = async (${2}) => {",
            "   ${3}",
            "}"
        ],
        "description": "for key value entries in dict"
    },
    "log": {
        "prefix": "log",
        "body": [
            "console.log(\"${1}\", ${1})"
        ],
        "description": "debug"
    },
    "type_variables": {
        "prefix": "type",
        "body": [
            "/** @type {${1}} */"
        ]
    }
}
